{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
          }
    },
    "ignorePatterns": ["webpack.config.js", "**/dist/*.js", "**/node_modules/*.js"],
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars" : "warn",
        "eqeqeq":"error",
        "no-console":"warn",
        "no-alert":"error"
    }
}
